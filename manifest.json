{
	
	"manifest_version": 2,

	"name": "Bulk Query",
	"description": "Bulk Query can perform multiple search queries in sequence.",
	"version": "1.0",

	"browser_action": {
		"default_icon": "icons/icon.png",
		"default_popup": "popup.html",
		"default_title": "Bulk Query"
	},

	"content_scripts": [ {

		"matches": ["https://www.google.com/*"],
		"js": ["js/jquery-3.2.1.min.js", "js/bootstrap.min.js"]

	}],

	"background": {
		"scripts": ["js/main.js"]
	}

}